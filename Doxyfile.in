# Generic doxygen settings as input for cmake to generate a Doxyfile

PROJECT_NAME            = "@DOXYGEN_PRODUCT_NAME@"
PROJECT_NUMBER          = "@DOXYGEN_PROJECT_NUMBER@"
INPUT                   = @DOXYGEN_INPUT@
OUTPUT_DIRECTORY        = @DOXYGEN_OUTPUT_DIRECTORY@
PROJECT_LOGO            = @DOXYGEN_CUSTOM_RESOURCES_DIR@/stackforce_logo.svg
HTML_HEADER             = @DOXYGEN_CUSTOM_RESOURCES_DIR@/sf_header.html
HTML_FOOTER             = @DOXYGEN_CUSTOM_RESOURCES_DIR@/sf_footer.html
HTML_EXTRA_STYLESHEET   = @DOXYGEN_CUSTOM_RESOURCES_DIR@/bootstrap.min.css \
                          @DOXYGEN_CUSTOM_RESOURCES_DIR@/sf_stylesheet.css
DISABLE_INDEX           = NO
GENERATE_TREEVIEW       = YES
STRIP_FROM_PATH         = @CMAKE_SOURCE_DIR@
INLINE_SIMPLE_STRUCTS   = YES
HIDE_IN_BODY_DOCS       = YES
